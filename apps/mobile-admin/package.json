{
  "name": "mobile-admin",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "start:lan": "EXPO_PUBLIC_API_URL=http://$(ipconfig getifaddr en0 || ipconfig getifaddr en1):4000/api expo start --lan",
    "start:lan:clear": "EXPO_PUBLIC_API_URL=http://$(ipconfig getifaddr en0 || ipconfig getifaddr en1):4000/api expo start --lan -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android"
  },
  "dependencies": {
    "expo": "~54.0.33",
    "expo-constants": "~18.0.8",
    "expo-dev-client": "^6.0.20",
    "expo-device": "~8.0.6",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.8",
    "expo-notifications": "~0.31.4",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-webview": "13.15.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
